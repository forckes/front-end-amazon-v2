{
	"trailingComma": "none",
	"tabWidth": 2,
	"useTabs": true,
	"semi": false,
	"singleQuote": true,
	"jsxSingleQuote": true,
	"arrowParens": "avoid",
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^components/(.*)$",
		"^[./]",
		"layout/(.*)$",
		"ui/(.*)$",
		"assets/(.*)$",
		"config/(.*)$",
		"store/(.*)$",
		"hooks/(.*)$",
		"types/(.*)$",
		"utils/(.*)$",
		"api/(.*)$",
		"shared/(.*)$",
		"../(.*)$",
		"./(.*)$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true
}
